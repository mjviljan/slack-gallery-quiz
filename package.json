{
  "name": "slack-gallery-quiz",
  "version": "1.0.0",
  "description": "A quiz using Slack account images and asking the user to recognize the people",
  "main": "src/index.ts",
  "scripts": {
    "build": "parcel build src/index.html --no-cache",
    "deploy": "yarn build && firebase deploy",
    "start": "yarn firebase serve --only functions,hosting",
    "setup": "yarn install && cd functions && yarn install && yarn build && cd ..",
    "tsc:watch": "tsc -w"
  },
  "author": "mjviljan",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^5.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "dotenv": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "shuffle.ts": "^1.0.1"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@types/jest": "^26.0.14",
    "firebase": "^7.21.1",
    "firebase-tools": "^8.11.2",
    "less": "^3.12.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
