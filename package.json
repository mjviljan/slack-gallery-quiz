{
  "name": "slack-gallery-quiz",
  "version": "2.0.0",
  "description": "A quiz using Slack account images and asking the user to recognize the people",
  "main": "src/index.ts",
  "scripts": {
    "build": "parcel build src/index.html --no-cache",
    "deploy": "yarn build && firebase deploy",
    "start": "yarn firebase serve --only functions,hosting",
    "setup": "yarn install && cd functions && yarn install && yarn build && cd ..",
    "tsc:watch": "tsc -w"
  },
  "author": "mjviljan",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^5.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "dotenv": "^16.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shuffle.ts": "^1.0.1"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@types/node": "^20.4.5",
    "firebase": "^10.1.0",
    "firebase-tools": "^12.4.6",
    "less": "^4.1.3",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  }
}
