{
  "name": "slack-gallery-quiz",
  "version": "0.1.0",
  "description": "A quiz using Slack account images and asking the user to recognize the people",
  "main": "src/index.ts",
  "scripts": {
    "build": "parcel build src/index.html --no-cache",
    "deploy": "yarn build && firebase deploy",
    "start": "yarn firebase serve --only functions,hosting",
    "setup": "yarn install --ignore-engines && cd functions && yarn install && yarn build && cd ..",
    "tsc:watch": "tsc -w"
  },
  "author": "mjviljan",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^4.8.0",
    "@types/dotenv": "^6.1.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "dotenv": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^3.1.2",
    "shuffle.ts": "^1.0.1"
  },
  "devDependencies": {
    "@google-cloud/functions-emulator": "^1.0.0-beta.5",
    "@types/jest": "^23.3.9",
    "firebase": "^5.7.2",
    "firebase-tools": "^6.2.2",
    "less": "^3.9.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "1.16.4",
    "typescript": "^3.1.6"
  }
}
